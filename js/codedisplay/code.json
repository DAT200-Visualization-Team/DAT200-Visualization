      "enqueue": {
        "header": "Queue.prototype.enqueue = function(x) {",
        "lines": [
          "  if (this.currentSize === this.theArray.length) {",
          "      this.doubleQueue();",
          "  }",
          "  this.back = this.increment(this.back);",
          "  this.theArray[this.back] = x;",
          "  this.currentSize++;"
        ]
      },
