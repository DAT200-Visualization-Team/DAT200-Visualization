      "removeByData": {
        "header": "LinkedList.prototype.removeByData = function(data) {",
        "lines": [ "    var pos = this.findPos(data);", "    if (pos == -1) {", "        return false;", "    }", "    else {", "        this.removeNode(pos);", "        return true;", "    }", "};" ]
